Print
	mxctl.control scr_cap

control + Print
	mxctl.control scr_cap_sel

control + alt + bracketright
	mxctl.control brightness_up

control + alt + bracketleft
	mxctl.control brightness_down

control + bracketright
	ictl vol_up

control + bracketleft
	ictl vol_down

control + backslash
	ictl vol_mute

alt + h 
  parcellite 

#
# multimedia keys
#
@XF86AudioRaiseVolume
	ictl vol_up

@XF86AudioLowerVolume
	ictl vol_down

@XF86AudioMute
	ictl vol_mute

@XF86MonBrightnessUp
	mxctl.control brightness_up

@XF86MonBrightnessDown
	mxctl.control brightness_down

# 
# menu shortcuts
#
super + d
	ictl dmenu_run

super + w
	ictl dmenu_window

super + q
	ictl dmenu_exit

control + alt + Delete
	ictl dmenu_exit

control + alt + l
	mxctl.control scr_lock

control + alt + 2
	ictl dmenu_video

control + alt + 3
	ictl dmenu_audio_sink

control + alt + 9
	mxctl.control dmenu_misc

